generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Contact {
  id         String  @id @default(cuid())
  firstName  String
  middleName String?
  lastName   String
  address1   String?
  address2   String?
  city       String?
  state      String?
  zip        String?
  phone      String?
  email      String?

  @@map(name: "contacts")
}
